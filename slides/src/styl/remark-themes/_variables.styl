$brand = #0d0d26
$brand-alt = #02617e
$brand-context = #02617e

$brand-dark-gray = #5b5149;
$brand-light-gray = #a69b91;
$brand-white-text = #fdfcfc

$brand-alt-light = lighten($brand-light-gray, 90%)
$brand-highlight = lighten($brand-alt-light, 60%)


$font = "Catamaran"
$code-font = "Ubuntu Mono"

$context-current = $brand-context
$context-current-minus-one = desaturate($brand-context, 40%)
$context-current-minus-two = desaturate($brand-context, 80%)
